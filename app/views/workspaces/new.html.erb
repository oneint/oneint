<div class='row'>
  <div class='col-sm-10'>
    <h4>New Workspace</h4>
    <%= form_for @workspace, class: 'form' do |f| %>
      <div class='form-group row'>
        <%= f.label :name, "Workspace name", class: 'col-sm-4 col-form-label'%>
        <%= f.text_field :name, class: 'form-control col-sm-6'%>
      </div>
      <%= f.submit 'Save', class: "btn btn-primary mb-2"%>
    <% end %>
  </div>
</div>
